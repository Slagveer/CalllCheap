<?xml version="1.0" encoding="UTF-8"?><Graphic version="2.0" xmlns="http://ns.adobe.com/fxg/2008"    scaleGridTop="1" scaleGridBottom="64" scaleGridLeft="4" scaleGridRight="82" >        <!-- highlight border leading -->    <Rect x="0" y="1" width="1" height="63">        <fill>            <LinearGradient x="0" scaleX="63" rotation="90">                <GradientEntry color="#ffffff" ratio="0" alpha=".25"/>                <GradientEntry color="#ffffff" ratio="1" alpha=".1"/>            </LinearGradient>        </fill>    </Rect>        <!-- separator dark -->    <Rect x="1" y="0" width="1" height="65">        <fill>            <SolidColor color="#1790B6" alpha="0.8"/>        </fill>    </Rect>		<!-- highlight border left-->    <Rect x="3" y="1" width="1" height="63">        <fill>            <LinearGradient x="0" scaleX="63" rotation="90">                <GradientEntry color="#52C2E7" ratio="0" alpha=".25"/>                <GradientEntry color="#52C2E7" ratio="1" alpha=".1"/>            </LinearGradient>        </fill>    </Rect>	<!-- invisible fix for scaling -->	<Rect x="0" y="0" width="83" height="65">		<fill>			<SolidColor color="#ffffff" alpha="0"/>		</fill>	</Rect>	</Graphic>